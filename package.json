{
  "name": "drizzle-relations-pg",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "update": "npm i next@latest react@latest react-dom@latest",
    "generate": "drizzle-kit generate",
